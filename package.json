    "chalk": "^4.1.1",
    "error-overlay-webpack-plugin": "^1.1.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^28.1.3",
    "jest-util": "^28.1.3",
    "jsx-to-string": "^1.4.0",
    "learnpack": "^0.1.14",
    "prettier": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
