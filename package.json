{
  "name": "learnpack-react",
  "description": "React compiler for learnpack tutorials",
  "version": "0.0.13",
  "author": "Alejandro Sanchez @alesanchezr",
  "bugs": "https://github.com/learnpack/learnpack/issues",
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chalk": "^4.1.1",
    "error-overlay-webpack-plugin": "^0.4.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^24.8.0",
    "jest-util": "^27.0.2",
    "jsx-to-string": "^1.4.0",
    "prettier": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "shelljs": "^0.8.4",
    "source-map-support": "^0.5.19",
    "webpack": "^5.40.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/test": "^1.2.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/learnpack/node-compiler",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "https://learnpack.co/terms",
  "oclif": {
    "commands": "./src/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "action": "./src/plugin.js"
    }
  },
  "repository": "learnpack/node-compiler",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
